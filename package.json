{
  "name": "cronflow-agnet",
  "code": "agnet",
  "version": "1.0.0",
  "description": "cronflow agnet to handle cronjobs that triggered",
  "main": "dist/index.js",
  "scripts": {
    "lint": "esw -w src test",
    "test": "jest --watch",
    "dev": "nodemon --ignore plugins --ignore public src/index.js --exec \"node -r babel-register\"",
    "local": "docker-compose -f docker-compose.dev.yml up",
    "taobao": "npm i --no-package-lock --registry=https://registry.npm.taobao.org",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "start": "node dist/index.js",
    "prestart": "npm run -s build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viaflow/agent.git"
  },
  "author": "Guo Tuo <guotuo1024@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/viaflow/agent/issues"
  },
  "homepage": "https://github.com/viaflow/agent#readme",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ],
    "ignore": [
      "plugins"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-watch": "^4.0.2",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3"
  },
  "dependencies": {
    "alphabetjs": "^0.1.4",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "colors": "^1.3.2",
    "dayjs": "^1.7.5",
    "ioredis": "^4.0.0",
    "lodash": "^4.17.10",
    "mysql2": "^1.6.1",
    "sequelize": "^4.38.0",
    "tracer": "^0.9.1"
  }
}
